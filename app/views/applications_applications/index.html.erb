<h1>Listing applications_applications</h1>

<table>
  <tr>
    <th>Applications event</th>
    <th>Name</th>
    <th>Birth year</th>
    <th>Course</th>
    <th>Comment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @applications_applications.each do |applications_application| %>
  <tr>
    <td><%= applications_application.applications_event_id %></td>
    <td><%= applications_application.name %></td>
    <td><%= applications_application.birth_year %></td>
    <td><%= applications_application.course %></td>
    <td><%= applications_application.comment %></td>
    <td><%= link_to 'Show', applications_application %></td>
    <td><%= link_to 'Edit', edit_applications_application_path(applications_application) %></td>
    <td><%= link_to 'Destroy', applications_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

